repeat task.wait() until game:GetService("Players").LocalPlayer
repeat task.wait() until game:IsLoaded()

task.spawn(function() 
    while true do 
        local r = reuqest({
            Url = "http://127.0.0.1:4861/?username=" . game.Players.LocalPlayer.Name,
            Method = "GET",
        })
        
        if r then 
            warn(r.Body)
        end
        task.wait(10)
    end
end)
